<% @questions.each do |question|%>
      <h3 style="font-size:18px; line-height:1px"><%= link_to question_path(question.id) do %>
        <%=question.title%></h3>
      <%end%>
    <%owner = User.find_by(id: question.user_id)%>
    <div class="panel-body" style="font-size:12px; line-height:1px">creado por <%= owner.email %> el d√≠a <%=question.created_at%></div>
    <hr style="color: #0056b2;" />
<% end %>

<%if current_user %>
  <a href="/questions/new" class="btn btn-lg btn-success fixed-bottom" style="position: absolute; bottom: 0; right:0; margin: 10px;">Has una pregunta!</a>
<% end %>
