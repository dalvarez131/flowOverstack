  <div class="panel panel-default">
    <div class="panel-heading">
      <h3><%= link_to question_path(question.id) do %>
        <%=question.title%></h3>
      <%end%>
    </div>
    <%owner = User.find_by(question.user_id)%>
    <div class="panel-body">creado por <%= owner.email %> el dÃ­a <%=question.created_at%></div>
  </div>
<% end %>

